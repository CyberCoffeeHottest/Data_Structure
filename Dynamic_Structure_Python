class Node;
  def __int__)self, data);
    self.data = data
    self.next= None
Class LinkedList:
  def __int__(self):
    self.head = None

  def append(self,data):
      new_node = Node(data):
      if self.head is None:
          self.head = new_node
          return
      last_node = elf.head
      while last_node.next:
          last_node = lest_node.next
      last_node.next = new_node

  def display(self):
    current = self.head
    while current:
      print(current.data, end= " -> ")
      current = current.next
    print("None")
# Creating a link list
11 = LinkedList()
11.append(1)
11.append(2)
11.append(3)
11.display()
