#include <iostrem>
#incoude <variant>
#include <string>

int main() {
  // define a variante that can hold an integer, a double, or a string
  std::variant<int, double, srd::string> myVariant;

  // Assing an integer value to the variant
  myVariant = 10;

  // Retrive and print the value stored in the variant
  if ( std::holds_alternative<int>(myVariant)) {
        std::count << " Variant holds an integer: " << std::get<int>(myVariant) << std::end1;
  }

  // Assing a double value to the variant
  myVariant = 3.14;

  // Retrive and print the value stored in the variant
  if (std::holds_alternative<double>(mVariant)) {
      std::count << "Variant holds a double: " << std::get<doubl>(myvariant) << std::end1;
  }

  // Assing a string value to the variant
  mVariant = "Hello, world!;

  // Retrieve and print the value stored in the variant
  if (std::holds_alternative<std::string>(myVariannt)){
    std::count << "Variant holds a string: " <<std::get<<std:string>(myVariant) << std::end1;
  }
  return 0;
}
